    <style name="Widget.GoogleMaterial.NavigationView" parent="@style/Widget.MaterialComponents.NavigationView">
        <item name="enforceMaterialTheme">true</item>
        <item name="itemBackground">@null</item>
        <item name="itemShapeAppearanceOverlay">@style/MaterialShape</item>
        <item name="itemShapeInsetBottom">@null</item>
        <item name="itemShapeInsetEnd">10dp</item>
        <item name="itemShapeInsetStart">@null</item>
        <item name="itemShapeInsetTop">@null</item>
        <item name="itemPadding">30dp</item>
        <item name="background">@color/material_background</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:textSize">16.5sp</item>
        <item name="android:lineSpacingExtra">1.5sp</item>
    </style>

    <style name="MaterialShape">
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">23.5dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">23.5dp</item>
    </style>

    <style name="Widget.GoogleMaterial.BottomNavigationView" parent="@style/Widget.MaterialComponents.BottomNavigationView">
        <item name="enforceMaterialTheme">true</item>
        <item name="itemBackground">@null</item>
        <item name="itemShapeAppearanceOverlay">@style/MaterialBottomShape</item>
        <item name="background">@color/material_card_background</item>
        <item name="android:textSize">15sp</item>
        <item name="android:lineSpacingExtra">1.5sp</item>
    </style>

    <style name="MaterialBottomShape">
        <item name="cornerSizeBottomLeft">30dp</item>
        <item name="cornerSizeBottomRight">30dp</item>
        <item name="cornerSizeTopLeft">30dp</item>
        <item name="cornerSizeTopRight">30dp</item>
    </style>

    <style name="TextStyleMain">
        <item name="android:textAppearance">?textAppearanceSubtitle1</item>
        <item name="android:textSize">16sp</item>
        <item name="android:lineSpacingExtra">1sp</item>
        <item name="android:fontFamily">@font/google_sans</item>
    </style>

    <style name="TextStyleMain_sec">
        <item name="android:textAppearance">?textAppearanceSubtitle1</item>
        <item name="android:fontFamily">@font/google_sans</item>
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingExtra">1sp</item>
    </style>

    <style name="TextStyleSummary">
        <item name="android:textAppearance">?textAppearanceBody2</item>
        <item name="android:textColor">?android:textColorSecondary</item>
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingExtra">2sp</item>
    </style>

    <style name="TextStyleSummary_pri">
        <item name="android:textAppearance">?textAppearanceBody2</item>
        <item name="android:textColor">?android:textColorSecondary</item>
        <item name="android:textSize">16sp</item>
        <item name="android:lineSpacingExtra">1sp</item>
    </style>

    <style name="myActionButtonStyle" parent="Widget.AppCompat.ActionButton">
        <item name="android:minWidth">36dp</item>
        <item name="android:minHeight">36dp</item>
    </style>

    <style name="toolbar_style">
        <item name="android:textAppearance">?textAppearanceBody2</item>
        <item name="android:textColor">?android:textColorSecondary</item>
        <item name="android:textSize">17sp</item>
        <item name="android:lineSpacingExtra">1sp</item>
    </style>

    <style name="NProgressBar" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:maxHeight">50dip</item>
        <item name="android:minHeight">5dip</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">
            @android:drawable/progress_indeterminate_horizontal
        </item>
        <item name="android:progressDrawable">@drawable/progress</item>
    </style>

    <!--实现BottomSheetDialog圆角效果-->
    <style name="BottomSheetDialog" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/bottomSheetStyleWrapper</item>
    </style>
    <style name="bottomSheetStyleWrapper" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="BottomSheetDialogStyle" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <style name="BottomNavStyle" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="iconSize">48dp</item>
    </style>

</resources>