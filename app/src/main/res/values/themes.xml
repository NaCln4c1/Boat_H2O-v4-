<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Boat_H2O2_Main" parent="Theme.MaterialComponents.Light">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/blue_light_primary</item>
        <item name="colorPrimaryVariant">@color/blue_light_primaryDark</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue_light_primary</item>
        <item name="colorSecondaryVariant">@color/blue_light_primaryDark</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/theme_status_bar_color_empty</item>
        <!-- Customize your theme here. -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="navigationViewStyle">@style/Widget.GoogleMaterial.NavigationView</item>
        <item name="android:navigationBarColor" tools:targetApi="l">@color/theme_window_color</item>
        <item name="android:windowLightNavigationBar">true</item>
    </style>

    <style name="AppTheme" parent="Theme.MaterialComponents.Light">
        <!-- Color Theme: https://material.io/develop/android/theming/color/ -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">@color/material_blue_light</item>
        <item name="colorSecondaryVariant">@color/material_background</item>
        <item name="colorOnSecondary">@color/white</item>

        <item name="android:windowLightStatusBar">true</item>
        <item name="navigationViewStyle">@style/Widget.GoogleMaterial.NavigationView</item>
        <item name="android:navigationBarColor">@color/material_background</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="android:windowBackground">@color/material_background</item>
        <item name="cardForegroundColor">@color/material_blue</item>
        <item name="android:statusBarColor">@color/material_background</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.MaterialComponents.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.MaterialComponents.Light" />

    <!--Custom themes-->
    <style name="AppBaseTheme" parent="Theme.MaterialComponents.Light">
        <!-- Color Theme: https://material.io/develop/android/theming/color/ -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorSecondary">@color/material_blue_light</item>
        <item name="colorSecondaryVariant">@color/material_blue_light</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="actionBarStyle">@style/MyActionBar</item>

        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowLightNavigationBar">true</item>
        <item name="navigationViewStyle">@style/Widget.GoogleMaterial.NavigationView</item>
        <item name="android:navigationBarColor">@color/material_card_background</item>
        <item name="android:windowBackground">@color/material_background</item>
        <item name="cardForegroundColor">@color/material_blue</item>
        <item name="android:statusBarColor">@color/material_background</item>
    </style>

    <style name="AppBaseTheme.NoActionBar.Settings">
        <item name="android:lineSpacingExtra">1.5sp</item>
    </style>

    <style name="MyActionBar" parent="@style/Widget.AppCompat.ActionBar">
        <item name="android:background">@color/material_background</item>
    </style>

    <style name="AppBaseTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

